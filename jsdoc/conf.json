{
	"source": {
		"include": [
			"."
		],
		"exclude": [
			"test"
		]
	},
	"templates": {
		"default": {
			"layoutFile": "jsdoc/layout.tmpl",
			"staticFiles": {
				"include": [
					"jsdoc/version-warning.css"
				]
			}
		}
	},
	"opts": {
		"destination": "doc",
		"readme": "README.md"
	},
	"plugins": [
		"plugins/markdown"
	]
}
